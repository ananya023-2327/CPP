// EXp -3 : WAP to check what quadrant a pair of coordinates belong to
#include <iostream>
using namespace std;
int main() {
  int x,y ;
  cout<<"Enter the values of x and y coordinates: ";
  cin>>x>>y;
  if (x>0 && y>0){
      cout<<"The given coordinates belong to quadrant 1";
  }
 else if (x<0 && y>0){
      cout<<"The given coordinates belong to quadrant 2";
  }
  else if (x<0 && y<0){
      cout<<"The given coordinates belong to quadrant 3";
  }
  else if(x>0 && y<0) {
      cout<<"The given coordinates belong to quadrant 4";
  }
  
  return 0;
}